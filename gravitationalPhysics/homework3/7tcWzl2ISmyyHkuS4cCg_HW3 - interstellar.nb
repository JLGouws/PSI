Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
     "We", " ", "set", " ", "a", " ", "deiscrete", " ", "grid", " ", "of", 
      " ", "values", " ", "for", " ", 
      RowBox[{"Psi", "[", 
       RowBox[{"b", ",", "6"}], "]"}], " ", "for", " ", "values", " ", "of", 
      " ", "b", " ", "from", " ", "bmin"}], "+", 
     RowBox[{"0.2", " ", 
      RowBox[{"(", 
       RowBox[{"for", " ", "numerical", " ", "reasons"}], ")"}], " ", "to", 
      " ", "bmax", " ", "with", " ", "a", " ", "step", " ", "of", " ", "0.01",
       " ", "for", " ", "the", " ", "second", " ", "branch"}]}], ",", " ", 
    RowBox[{
    "and", " ", "then", " ", "reverse", " ", "the", " ", "table", " ", 
     "because", " ", "the", " ", "function", " ", "is", " ", "descending"}]}],
    " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"psi2", " ", "=", " ", 
    RowBox[{"Reverse", "[", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
