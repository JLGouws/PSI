Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "We", " ", "set", " ", "a", " ", "discrete", " ", "grid", " ", "of", " ", 
     "values", " ", "for", " ", 
     RowBox[{"Psi", "[", 
      RowBox[{"b", ",", "6"}], "]"}], " ", "for", " ", "values", " ", "of", 
     " ", "b", " ", "from", " ", "bmin", " ", "to", " ", "bmax", " ", "with", 
     " ", "a", " ", "step", " ", "of", " ", "0.01", " ", "for", " ", "the", 
     " ", "first", " ", 
     RowBox[{"branch", ".", " ", "The"}], " ", "values", " ", "of", " ", 
     "Psi", " ", "are", " ", "saved", " ", "in", " ", "the", " ", "first", 
     " ", "line", " ", "and", " ", "the", " ", "corresponding", " ", "b", " ",
      "values", " ", "in", " ", "the", " ", "second", " ", 
     RowBox[{"line", ".", " ", "We"}], " ", "will", " ", "do", " ", "the", 
     " ", "same", " ", "for", " ", "the", " ", "second", " ", "branch", " ", 
     "and", " ", "then", " ", "we", " ", 
     RowBox[{"can", " ", "'"}], 
     RowBox[{"reverse", "'"}], " ", "the", " ", "function", " ", "in", " ", 
     "the", " ", "sense", " ", "that", " ", "we", " ", "know", " ", "the", 
     " ", 
     RowBox[{"Psi", "'"}], "s", " ", "corresponding", " ", "to", " ", 
     RowBox[{"b", "'"}], "s", " ", "but", " ", "the", " ", "table", " ", 
     "can", " ", "also", " ", "give", " ", "the", " ", 
     RowBox[{"b", "'"}], "s", " ", "corresponding", " ", "to", " ", 
     RowBox[{"Psi", "'"}], "s"}], ",", " ", 
    RowBox[{"which", " ", "is", " ", "single", " ", 
     RowBox[{"valued", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"psi1", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Psi", "[", 
         RowBox[{"b", ",", "6"}], "]"}], ",", "b"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"b", ",", "bmin", ",", " ", "bmax", ",", " ", "0.01"}], 
       "}"}]}], "]"}]}], ";"}]}]], "Input",
 CellChangeTimes->{{3.9152902302392187`*^9, 3.9152903036141834`*^9}, {
  3.9155136229316807`*^9, 3.915513692109087*^9}, {3.9155139579286356`*^9, 
  3.9155140950824723`*^9}, {3.915514128605036*^9, 3.9155141326619873`*^9}, {
